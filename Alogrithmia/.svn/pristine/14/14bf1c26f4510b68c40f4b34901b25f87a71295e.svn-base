	this.respond_content = respond_content;
}
public String getRespond_time() {
	return respond_time;
}
public void setRespond_time(String respond_time) {
	this.respond_time = respond_time;
}
public int getFlag() {
	return flag;
}
public void setFlag(int flag) {
	this.flag = flag;
}
public String getReviewer_name() {
	return reviewer_name;
}
public void setReviewer_name(String reviewer_name) {
	this.reviewer_name = reviewer_name;
}
public String getResponer_name() {
	return responer_name;
}
public void setResponer_name(String responer_name) {
	this.responer_name = responer_name;

package alg.base.bean;

public class Respond {
int respond_id;
int review_id;
int algorithm_id;
int reviewer_id;
int responer_id;
String reviewer_name;
String responer_name;
int flag;
String respond_content;
String respond_time;
public int getRespond_id() {
	return respond_id;
}
public void setRespond_id(int respond_id) {
	this.respond_id = respond_id;
}
public int getReview_id() {
	return review_id;
}
public void setReview_id(int review_id) {
	this.review_id = review_id;
}
public int getAlgorithm_id() {
	return algorithm_id;
}
public void setAlgorithm_id(int algorithm_id) {
	this.algorithm_id = algorithm_id;
}
public int getReviewer_id() {
	return reviewer_id;
}
public void setReviewer_id(int reviewer) {
